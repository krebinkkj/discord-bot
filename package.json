    "eslint": "^8.56.0",
    "eslint-plugin-jsdoc": "^46.4.3",
    "node": "^18.18.2",
    "nodemon": "^3.0.3",
    "prettier": "3.2.5"
  },
  "keywords": [
    "discord",
    "discord.js",
    "discord-bot",
    "open-source",
    "bot",
    "multipurpose",
    "multi-purpose-bot"
  ],
  "_moduleAliases": {
    "@root": ".",
    "@handlers": "src/handlers/",
    "@helpers": "src/helpers/",
    "@schemas": "src/database/schemas/",
    "@src": "src/",
    "@structures": "src/structures/"
