    "node-fetch": "^2.7.0",
    "os": "^0.1.2",
    "pino": "^8.18.0",
    "pino-pretty": "^10.3.1",
    "pretty-ms": "^7.0.1",
    "snakecord": "^1.0.9",
    "sourcebin_js": "^0.0.3-ignore",
    "string-progressbar": "^1.0.4",
    "table": "^6.8.1",
    "timestamp-to-date": "^1.1.0",
    "twemoji-parser": "^14.0.0"
